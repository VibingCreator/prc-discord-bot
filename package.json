{
  "name": "prc-discord-bot",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot designed to meet the specific needs of the customer.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "ts-node-dev -r dotenv/config --files ./src/index.ts",
    "build": "tsc --noEmitOnError",
    "lint": "eslint ./src/**/*.ts",
    "fix": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write ./src/**/*.ts"
  },
  "dependencies": {
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "gitea-api": "^1.17.3-1",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VibingCreator/prc-discord-bot.git"
  },
  "author": "Rados≈Çaw Miluk <VibingCreator@gmail.com> (https://vibingcreator.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VibingCreator/prc-discord-bot/issues"
  },
  "homepage": "https://github.com/VibingCreator/prc-discord-bot#readme"
}
